{
    "manifest_version": 2,
    "name": "Github-QL",
    "version": "1.0",

    "description": "Github quality of life functions.",
    "homepage_url": "https://github.com/cepix1234/github-QL-browser-extension",

    "icons": {
        "48": "icons/github-48.png"
    },
    "permissions": ["activeTab"],

    "browser_action": {
        "default_icon": "icons/github-48.png",
        "default_title": "Github-QL",
        "default_popup": "popup/actions.html"
    }
}

